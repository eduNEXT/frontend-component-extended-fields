{
	"name": "@edunext/frontend-component-extended-fields",
	"version": "1.0.1-alpha",
	"description": "Site extended profile Plugincomponent utilizing Frontend Plugin Framework",
	"main": "src/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/edunext/frontend-component-extended-fields.git"
	},
	"browserslist": [
		"extends @edx/browserslist-config"
	],
	"scripts": {
