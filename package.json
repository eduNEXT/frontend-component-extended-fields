	},
	"peerDependencies": {
		"@edx/frontend-platform": "^7.0.0 || ^8.0.0",
		"@openedx/frontend-plugin-framework": "^1.0.0",
		"@openedx/paragon": "^22.0.0 || ^23.0.0",
